!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
A	Base.hpp	/^class A : public Base {};$/;"	c
B	Base.hpp	/^class B : public Base {};$/;"	c
BASE_HPP	Base.hpp	/^# define BASE_HPP$/;"	d
Base	Base.hpp	/^class Base$/;"	c
C	Base.hpp	/^class C : public Base {};$/;"	c
CXX	Makefile	/^CXX				=	clang++$/;"	m
NAME	Makefile	/^NAME			=	Identyfy$/;"	m
OBJS	Makefile	/^OBJS			=	$(addprefix $(OBJS_DIR)\/, $(SRCS:.cpp=.o))$/;"	m
OBJS_DIR	Makefile	/^OBJS_DIR		=	.\/objs$/;"	m
RM	Makefile	/^RM				=	rm -f$/;"	m
SRCS	Makefile	/^SRCS			=	main.cpp\\$/;"	m
generate	Base.cpp	/^Base *	generate( void )$/;"	f
identify	Base.cpp	/^void	identify( Base& p )$/;"	f
identify	Base.cpp	/^void	identify( Base* p )$/;"	f
main	main.cpp	/^int main( void )$/;"	f
operator <<	Base.cpp	/^std::ostream &	operator<<( std::ostream & o, Base const & i )$/;"	f
~Base	Base.hpp	/^		virtual ~Base() {}$/;"	f	class:Base
